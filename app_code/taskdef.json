{
   "executionRoleArn":"arn:aws:iam::648089517491:role/boilerplate-ecsTaskExecutionRole",
   "taskRoleArn":"arn:aws:iam::648089517491:role/boilerplate-ecsTaskRole",
   "containerDefinitions": [
      {
         "name":"boilerplate-container",
         "image":"<IMAGE1_NAME>",
         "portMappings":[
            {
               "containerPort":80,
               "hostPort":80,
               "protocol":"tcp"
            }
         ],
         "essential":true,
         "cpu":512,
         "memoryReservation":256,
         "secrets": [
            {
               "name": "BOUND_ADDRESS",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/BOUND_ADDRESS"
            },
            {
               "name": "FRONTEND_ORIGIN",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/FRONTEND_ORIGIN"
            },
            {
               "name": "VALID_ORIGIN_VALUE",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/VALID_ORIGIN_VALUE"
            },
            {
               "name": "VALID_REFERER_VALUE",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/VALID_REFERER_VALUE"
            },
            {
               "name": "DATABASE_URL",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/DATABASE_URL"
            },
            {
               "name": "REDIS_ADDRESS_PORT",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/REDIS_ADDRESS_PORT"
            },
            {
               "name": "REDIS_PRIVATE_KEY",
               "valueFrom": "arn:aws:ssm:ap-northeast-1:648089517491:parameter/REDIS_PRIVATE_KEY"
            }
         ],
         "logConfiguration": {
            "logDriver": "awslogs",
            "secretOptions": null,
            "options": {
               "awslogs-group": "/ecs/boilerplate-ecs-log-group",
               "awslogs-region":  "ap-northeast-1",
               "awslogs-stream-prefix": "boilerplate-ecs"
            }
         }
      }
   ],
   "requiresCompatibilities":[
      "FARGATE"
   ],
   "networkMode":"awsvpc",
   "cpu":"512",
   "memory":"1024",
   "family":"boilerplate-app-task-definition"
}